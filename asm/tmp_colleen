section .data
    msg db "section .data", 10, "    msg db ""section .data"", 10, "    code db ""
section .text
global _start
_start:
    mov eax, 4
    mov ebx, 1
    mov ecx, msg
    mov edx, msglen
    int 0x80
    mov eax, 4
    mov ebx, 1
    mov ecx, code
    mov edx, codelen
    int 0x80
    mov eax, 1
    xor ebx, ebx
    int 0x80
section .text
global _start
_start:
    mov eax, 4
    mov ebx, 1
    mov ecx, msg
    mov edx, msglen
    int 0x80
